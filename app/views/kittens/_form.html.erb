<%= form_for @kitten do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <br>
  <%= f.label :cuteness %>
  <%= f.select :cuteness, ['adorable', 'cute', 'precious'] %>
  <br>
  <%= f.label :softness %>
  <%= f.select :softness, ['silky', 'fluffy', 'cuddly'] %>
  <br>
  <%= f.submit "Create Kitten" %>
<% end %>
